#include <iostream>
using namespace std;

struct Node{
    int data;
    Node* next;
};
Node* head;

void insert(int x){
    Node* temp1=new Node();
    temp1->data=x;
    temp1->next=NULL;
    if(head==NULL){
        head=temp1;
        return;
    }
    Node* temp2=head;
    while(temp2->next!=NULL){
        temp2=temp2->next;
    }
    temp2->next=temp1;
    temp1->next=NULL;
}
void print(){
    Node* temp=head;
    while(temp!=NULL){
        cout << (temp->data) << " ";
        temp=temp->next;
    }
    cout << endl;
}
void Delete(int n){
    Node* temp3=head;
    if(n==1)
    {
        head=temp3->next;
        delete temp3;
        return;
    }
    for(int i=0;i<n-2;i++){
        temp3=temp3->next;  //temp3 points to (n-1)th node
    }
    Node* temp5=temp3->next; // nth Node
    temp3->next=temp5->next;
    delete temp5;
}
int main(){
    head=NULL;
    int n;
    cin >> n;
    for(int i=1;i<=n;i++){
        int x;
        cin >> x;
        insert(x);
    }
    print();
    int k;
    cin >> k;
    for(int i=1;i<=k;i++){
        int x;
        cin >> x;
        Delete(x);
        print();
    }
}
