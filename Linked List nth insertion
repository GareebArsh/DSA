#include <iostream>
using namespace std;

struct Node{
    int data;
    Node* next;
};
Node* head;

void insert(int x,int y){
    Node* temp1=new Node();
    temp1->data=x;
    temp1->next=NULL;
    if(y==1){
        temp1->next=head;
        head=temp1;
        return;
    }
    Node* temp2=head;
    for(int i=0;i<y-2;i++){
        temp2=temp2->next;
    }
    temp1->next=temp2->next;
    temp2->next=temp1;
}

void print(){
    Node* temp=head;
    while(temp!=NULL){
        cout << temp->data << " ";
        temp=temp->next;
    }
}


int main() {
	head=NULL;
	int n;
	cin >> n;
	for(int i=1;i<=n;i++){
	    int x,y;
	    cin >> x >> y;
	    insert(x,y);
	}
	print();
	return 0;
}
